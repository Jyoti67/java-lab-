import java.util.Scanner;
public class errorCheaking
{
	public static void main(String[] args) {
	    Scanner sc= new Scanner(System.in);
	 
	    try{
	        int x=10;
	        int y=0;
	        int z=x/y;
	        System.out.println(z);
	       // System.out.println(arr[3]);
	    }
	    catch(Exception err){
	        System.out.println(err.getMessage());
	    }
	    
	}
}
