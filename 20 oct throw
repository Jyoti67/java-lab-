public class A{
   public static void cheakNum(int n){
       if(n<1){
           throw new ArithmeticException("\n number is negative cant calculate suare");
           
       }else{
           System.out.println("Square of num"+n+"is:"+(n*n));
       }
   } 

public static void main(String[] args) {
    A th=new A();
    th.cheakNum(-3);
		System.out.println("Rest of the code....");
	}

}
