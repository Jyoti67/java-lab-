public class A{
   public static int N(int m,int n) throws ArithmeticException{
       int div=m/n;
       return div;
}
public static void main(String[] args) {
    A ob=new A();
    try{
        System.out.println(ob.A(45,0));
    }
    catch(ArithmeticException e){
        System.out.println("Cant divided by 0");
    }
    System.out.println("Rest of the code");
	}

}
