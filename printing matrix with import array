import java.util.Arrays;
public class TwoDimensionalArray
{
	public static void main(String[] args) {
	    int a[][]=new int[3][3];
	    for(int i=0;i<a.length;i++){
	        System.out.println();
	        for(int j=0;j<a[i].length;j++){
	            a[i][j]=i+j;
	           
	           
	        }
	        
	    }
	    System.out.println(Arrays.deepToString(a));
	}
}
